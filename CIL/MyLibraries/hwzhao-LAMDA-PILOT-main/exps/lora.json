{
    "prefix": " ",
    "dataset": "cifar224",
    "memory_size": 0,
    "memory_per_class": 0,
    "fixed_memory": false,
    "shuffle": true,
    "init_cls": 10,
    "increment": 10,
    "model_name": "lora",
    "backbone_type": "pretrained_vit_b16_224_lora",
    "device": ["0"],
    "seed": [1993],

    "tuned_epoch": 0,
    "init_lr": 0.01,
    "batch_size": 256,
    "weight_decay": 0.05,
    "min_lr": 1e-8,
    "optimizer": "sgd",

    "lora_r": 16,
    "lora_alpha": 16,
    "target_modules":["q", "v"],
    "lora_dropout":0.1,
    "lora_bias": "none",
    "modules_to_save":["classifier"]
}

