{
    "prefix": " ",
    "dataset": "imagenetr",
    "memory_size": 0,
    "memory_per_class": 0,
    "fixed_memory": false,
    "shuffle": true,
    "init_cls": 40,
    "increment": 40,
    "model_name": "lora",
    "backbone_type": "pretrained_vit_b16_224_lora",
    "device": ["1"],
    "seed": [1993],

    "tuned_epoch": 10,
    "init_lr": 0.001,
    "batch_size": 24,
    "weight_decay": 0,
    "min_lr": 1e-5,
    "optimizer": "adam",
    "scheduler": "constant",
    "reinit_optimizer": true,

    "lora_r": 4,
    "lora_alpha": 8,
    "target_modules":["q", "v"],
    "lora_dropout":0.1,
    "lora_bias": "none",
    "modules_to_save":["classifier"]
}

